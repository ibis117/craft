{
    "name": "ibis117/craft",
    "description": "Rapid Development Package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanil Nakarmi",
            "email": "cresilsanil@gmail.com",
            "homepage": "https://sanilnakarmi.com.np"
        }
    ],
    "homepage": "https://github.com/ibis117/craft",
    "keywords": ["Laravel", "Craft"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ibis117\\Craft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibis117\\Craft\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibis117\\Craft\\CraftServiceProvider"
            ],
            "aliases": {
                "Craft": "Ibis117\\Craft\\Facades\\Craft"
            }
        }
    },
    "non-feature-branches": ["feature/.*", "develop"]
}
